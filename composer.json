{
	"name": "nwsnet/nws-municipal-statutes",
	"type": "typo3-cms-extension",
	"description": "Reads all available municipal statutes from the TSA FullRest interface.",
	"homepage": "https://www.die-netzwerkstatt.de/",
	"keywords": [
		"TYPO3",
		"extension",
		"legislator",
		"legal norm"
	],
	"autoload": {
		"psr-4": {
			"Nwsnet\\NwsMunicipalStatutes\\": "Classes"
		},
		"psr-0": {
			"SimpleHtmlDom": "Classes/Dom/"
		},
		"files": [
			"Classes/Dom/SimpleHtmlDom.php"
		]
	},
	"authors": [
		{
			"name": "Dirk Meinke",
			"email": "typo3@die-netzwerkstatt.de",
			"homepage": "https://www.die-netzwerkstatt.de/"
		}
	],
	"license": [
		"GPL-2.0-or-later"
	],
	"require": {
		"php": "^7.4 | ^8",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-pdo": "*",
		"ext-iconv": "*",
		"typo3/cms-core": ">=10.4.0, <= 12.4.99"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": "build/web",
			"extension-key": "nws_municipal_statutes"
		}
	}
}
